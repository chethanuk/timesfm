# TimesFM API Configuration

# Model Settings
MODEL_NAME=google/timesfm-2.5-200m-pytorch
MODEL_CACHE_DIR=./model_cache
DEVICE=cuda

# Server Settings
HOST=0.0.0.0
PORT=8000
WORKERS=1

# TimesFM Configuration
DEFAULT_MAX_CONTEXT=1024
DEFAULT_MAX_HORIZON=256
DEFAULT_NORMALIZE=true
DEFAULT_USE_QUANTILE_HEAD=true
DEFAULT_FORCE_FLIP_INVARIANCE=true
DEFAULT_INFER_IS_POSITIVE=true
DEFAULT_FIX_QUANTILE_CROSSING=true

# Performance
ENABLE_TORCH_COMPILE=true

# Logging
LOG_LEVEL=INFO
